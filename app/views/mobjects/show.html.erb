              				  <% end %>
              				<% end %>
                    </td>
                    <td>
                    <%= showImage2(:small, ca.user, true) %>
                    </td>
                      <td>
                        <smallcall>
                        <%= ca.user.name %><br>
                        <%= ca.user.lastname %>
                        </smallcal>
                      </td>
