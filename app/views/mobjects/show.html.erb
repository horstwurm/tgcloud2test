                      <smallcall>
                      <%= ca.date_from.strftime("%d.%m.%Y")+"  "+ca.time_from.to_s+ (I18n.t :uhr) %><br>
                      <%= ca.date_to.strftime("%d.%m.%Y")+"  "+ca.time_to.to_s+(I18n.t :uhr) %>
                      </smallcal>
                    </td>
                  </tr>
